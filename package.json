{
  "name": "gatsby-source-gcs-image",
  "description": "GatsbyJS plugin to source images from Google Cloud Storage",
  "version": "1.0.1",
  "author": "Parker Holladay <parker.holladay@gmail.com>",
  "dependencies": {
    "@google-cloud/storage": "6.9.4",
    "gatsby-source-filesystem": "5.7.0"
  },
  "devDependencies": {
    "@types/chai": "4.3.4",
    "@types/eslint": "8.21.1",
    "@types/mocha": "10.0.1",
    "@types/node": "18.15.0",
    "@types/react-dom": "18.0.11",
    "@types/sinon": "10.0.13",
    "@types/sinon-chai": "3.2.9",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "c8": "7.13.0",
    "chai": "4.3.7",
    "eslint": "8.35.0",
    "eslint-plugin-node": "11.1.0",
    "gatsby": "5.7.0",
    "husky": "8.0.3",
    "mocha": "10.2.0",
    "rimraf": "4.4.0",
    "sinon": "15.0.1",
    "sinon-chai": "3.7.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "keywords": [
    "cloud storage",
    "gatsby",
    "gatsby-plugin",
    "gatsby-source",
    "gcs"
  ],
  "license": "MIT",
  "peerDependencies": {
    "gatsby": "^5.0.0"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:compile",
    "build:clean": "rimraf ./*.js ./*.d.ts",
    "build:compile": "tsc",
    "build:copy": "cp package.json README.md LICENSE ./dist",
    "lint": "eslint src/ --max-warnings 0",
    "lint:ci": "eslint src/ --max-warnings 0 --format junit > eslint.xml",
    "prepare": "husky install",
    "start": "tsc -w",
    "test": "mocha --reporter nyan",
    "test:ci": "c8 --reporter text npm run test -- --reporter tap",
    "test:cover": "c8 npm run test",
    "test:watch": "npm run test -- --watch"
  }
}
